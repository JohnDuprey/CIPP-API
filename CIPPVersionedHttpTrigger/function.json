{
  "scriptFile": "../Modules/CippEntrypoints/CippEntrypoints.psm1",
  "entryPoint": "Receive-CippHttpTrigger",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": ["get", "post", "put", "patch", "delete"],
      "route": "{Version}/{CIPPEndpoint}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "name": "starter",
      "type": "durableClient",
      "direction": "in"
    }
  ]
}
